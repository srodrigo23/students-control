generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  name          String?
  email         String?   @unique
  emailVerified DateTime?
  image         String?
  id            Int       @id @default(autoincrement())
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model Student {
  codsis    Int
  lastname  String
  firstname String
  ci        String
  id        Int    @id @default(autoincrement())
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
